{
  "$schema": "http://json.schemastore.org/prettierrc",
  "trailingComma": "es5",
  "printWidth": 4,
  "tabWidth": 4,
  "useTabs": false,
  "semi": false,
  "singleQuote": false,
  "quoteProps": "consistent",
  "trailingComma": "es5",
  "bracketSpacing" : false,
  "arrowParens": "always",
  "requirePragma": false,
  "insertPragma": false,
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "off"
}
