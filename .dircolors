# these are basic filesystem items
NORMAL 00                   # normal
FILE 00                     # file
DIR 00;34                   # directory
LINK 00;36                  # link
MULTIHARDLINK 00            # hard link
FIFO 40;33                  # pipe
SOCK 00;35                  # socket
DOOR 00;35                  # door
BLK 40;33;01                # block device
CHR 40;33;01                # character device
ORPHAN 01;05;37;41          # symbolic link pointing to a non-existent file
MISSING 01;05;37;41         # symbolic link (visible when you type ls -l)
SETUID 37;41                # file that is setuid (u+s)
SETGID 30;43                # file that is setgid (g+s)
CAPABILITY 30;41            # file with capability
STICKY_OTHER_WRITABLE 30;42 # directory that is sticky and other-writable
OTHER_WRITABLE 34;42        # directory that is other-writable (o+w) and not sticky
STICKY 37;44                # directory with the sticky bit set (+t) and not other-writable
EXEC 00;91                  # executable

