<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.jasonivey.update_mac_lock_screen</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/env</string>
        <string>bash</string>
        <string>-c</string>
        <string>$HOME/settings/update_mac_lock_screen.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartInterval</key>
    <integer>3600</integer>
    <key>UserName</key>
    <string>jasoni</string>
    <key>WorkingDirectory</key>
    <string>/Users/jasoni/settings</string>
    <key>StandardOutPath</key>
    <string>/tmp/update_mac_lock_screen.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/update_mac_lock_screen.err</string>
</dict>
</plist>
